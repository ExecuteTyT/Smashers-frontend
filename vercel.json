{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    { "src": "/assets/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/(.*\\.(png|jpg|jpeg|gif|svg|webp|PNG|JPG|JPEG))", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/training", "dest": "/training/index.html" },
    { "src": "/schedule", "dest": "/schedule/index.html" },
    { "src": "/contacts", "dest": "/contacts/index.html" },
    { "src": "/faq", "dest": "/faq/index.html" },
    { "src": "/privacy-policy", "dest": "/privacy-policy/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
